{
  "name": "typescript-plugin-lodash",
  "version": "0.1.0",
  "description": "A simple typescript transform to cherry-pick Lodash modules so you donâ€™t have to.",
  "main": "lib/index.js",
  "types": "src/index.ts",
  "files": [
    "lib",
    "src"
  ],
  "license": "WTFPL",
  "author": "Morlay Null <morlay.null@gmail.com>",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "json",
      "jsx",
      "js"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.es6"
    },
    "testRegex": ".*/__tests__/.+\\.(test|spec)\\.(ts|tsx)$"
  },
  "scripts": {
    "lint": "tslint -c tslint.json \"src/{,**/}{,**/}/*.ts{,x}\"",
    "jest": "jest",
    "build": "tsc",
    "test": "npm run lint && jest",
    "prepublish": "npm test && npm run build"
  },
  "devDependencies": {
    "@types/jest": "20.0.x",
    "jest": "20.0.x",
    "ts-jest": "20.0.14",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "typescript": "2.5.2"
  },
  "peerDependencies": {
    "typescript": ">=2.5"
  },
  "dependencies": {
    "@types/lodash": "4.14.x",
    "lodash": "4.17.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morlay/typescript-plugin-lodash.git"
  },
  "bugs": {
    "url": "https://github.com/morlay/typescript-plugin-lodash/issues"
  },
  "homepage": "https://github.com/morlay/typescript-plugin-lodash#readme"
}
